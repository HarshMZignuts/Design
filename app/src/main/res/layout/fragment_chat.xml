<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>

    </data>

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.chat.ChatFragment">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/Card_Blue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="27dp">

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/shapeableImageView12"
                android:layout_width="13dp"
                android:layout_height="13dp"
                android:layout_marginStart="18dp"
                android:layout_marginTop="26dp"
                android:src="@drawable/back_arrow_white"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/shapeableImageView13"
                style="@style/Circuler"
                android:layout_width="31dp"
                android:layout_height="31dp"
                android:layout_marginStart="23dp"
                android:src="@drawable/oval"
                app:layout_constraintBottom_toBottomOf="@+id/shapeableImageView12"
                app:layout_constraintStart_toEndOf="@+id/shapeableImageView12"
                app:layout_constraintTop_toTopOf="@+id/shapeableImageView12"/>

            <TextView
                android:id="@+id/textView50"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="11dp"
                android:fontFamily="@font/noto_sans"
                android:lineHeight="22sp"
                android:text="@string/martin_randolph"
                android:textColor="@color/white"
                android:textSize="18sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/shapeableImageView13"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/shapeableImageView13"
                app:layout_constraintTop_toTopOf="@+id/shapeableImageView13" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/textView51"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="160dp"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="160dp"
        android:text="@string/yesterday"
        android:textSize="12sp"
        android:lineHeight="16sp"
        android:textColor="@color/black3"
        android:fontFamily="@font/noto_sans"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView3" />

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/materialCardView11"
        style="@style/CustomCardViewStyl3"
        android:layout_width="207dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="58dp"
        android:layout_marginTop="17dp"
        android:backgroundTint="@color/card_back_gray"
        android:paddingStart="15dp"
        android:paddingTop="10dp"
        android:paddingEnd="12dp"
        android:paddingBottom="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView51">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/noto_sans"
                android:lineHeight="18sp"
                android:text="@string/what_is_the_most_popular_services"
                android:textColor="@color/black"
                android:textSize="14sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

            </TextView>
        </androidx.constraintlayout.widget.ConstraintLayout>

    </com.google.android.material.card.MaterialCardView>

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/materialCardView12"
        style="@style/CustomCardViewStyl4"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:backgroundTint="@color/card_back_gray"
        android:paddingStart="15dp"
        android:paddingTop="9.5dp"
        android:paddingEnd="23dp"
        android:paddingBottom="7.5dp"
        app:layout_constraintStart_toStartOf="@+id/materialCardView11"
        app:layout_constraintTop_toBottomOf="@+id/materialCardView11">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/noto_sans"
                android:lineHeight="18sp"
                android:text="@string/do_you_like_it"
                android:textColor="@color/black"
                android:textSize="14sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

            </TextView>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </com.google.android.material.card.MaterialCardView>

    <com.google.android.material.imageview.ShapeableImageView
        style="@style/Circuler"
        android:layout_width="34dp"
        android:layout_height="34dp"
        android:layout_marginStart="18dp"
        android:layout_marginEnd="6dp"
        android:src="@drawable/oval"
        app:layout_constraintEnd_toStartOf="@+id/materialCardView12"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/materialCardView12">

    </com.google.android.material.imageview.ShapeableImageView>

    <TextView
        android:id="@+id/textView52"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:fontFamily="@font/noto_sans"
        android:lineHeight="12sp"
        android:text="@string/_5_hrs_ago"
        android:textColor="@color/black3"
        android:textSize="9sp"
        app:layout_constraintEnd_toEndOf="@+id/textView51"
        app:layout_constraintStart_toStartOf="@+id/textView51"
        app:layout_constraintTop_toBottomOf="@+id/materialCardView12" />

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/materialCardView13"
        style="@style/CustomCardViewStyl5"
        android:layout_width="155dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="24dp"
        android:backgroundTint="@color/card_blue_2"
        android:paddingStart="17dp"
        android:paddingTop="9dp"
        android:paddingEnd="17dp"
        android:paddingBottom="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView52">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"

                android:fontFamily="@font/noto_sans"
                android:lineHeight="19sp"
                android:text="@string/i_think_top_two_are"
                android:textColor="@color/white"
                android:textSize="14sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

            </TextView>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </com.google.android.material.card.MaterialCardView>

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/shapeableImageView14"
        style="@style/CustomeImage"
        android:layout_width="110dp"
        android:layout_height="62dp"
        android:layout_marginTop="2dp"
        android:src="@drawable/rectangle"
        app:layout_constraintEnd_toEndOf="@+id/materialCardView13"
        app:layout_constraintTop_toBottomOf="@+id/materialCardView13">

    </com.google.android.material.imageview.ShapeableImageView>

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/shapeableImageView15"
        style="@style/CustomeImage2"
        android:layout_width="110dp"
        android:layout_height="62dp"
        android:layout_marginEnd="1dp"
        android:src="@drawable/rectangle_2"
        app:layout_constraintEnd_toStartOf="@+id/shapeableImageView14"
        app:layout_constraintTop_toTopOf="@+id/shapeableImageView14">

    </com.google.android.material.imageview.ShapeableImageView>

    <TextView
        android:id="@+id/textView53"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:fontFamily="@font/noto_sans"
        android:lineHeight="12sp"
        android:text="@string/_1_hr_ago"
        android:textColor="@color/black3"
        android:textSize="9sp"
        app:layout_constraintEnd_toEndOf="@+id/textView52"
        app:layout_constraintStart_toStartOf="@+id/textView52"
        app:layout_constraintTop_toBottomOf="@+id/shapeableImageView15" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingTop="8dp"
            android:paddingBottom="8dp">

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/materialCardView14"
                android:layout_width="41dp"
                android:layout_height="36dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="24dp"
                android:layout_marginBottom="8dp"
                android:backgroundTint="@color/Card_Blue"
                app:cardCornerRadius="10dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <com.google.android.material.imageview.ShapeableImageView
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:src="@drawable/vector_9"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                    </com.google.android.material.imageview.ShapeableImageView>
                </androidx.constraintlayout.widget.ConstraintLayout>

            </com.google.android.material.card.MaterialCardView>


            <EditText
                android:id="@+id/textInputEditText"
                android:layout_width="0dp"
                android:layout_height="wrap_content"


                android:layout_marginStart="18dp"
                android:layout_marginEnd="7dp"
                android:paddingHorizontal="20dp"

                android:background="@drawable/edite_text"
                android:fontFamily="@font/noto_sans"
                android:hint="@string/type_here"
                android:lineHeight="16sp"
                android:textColor="@color/black"
                android:textSize="12sp"
                app:layout_constraintBottom_toBottomOf="@+id/materialCardView14"
                app:layout_constraintEnd_toStartOf="@+id/materialCardView14"
                app:layout_constraintStart_toEndOf="@+id/shapeableImageView16"
                app:layout_constraintTop_toTopOf="@+id/materialCardView14"
                tools:ignore="TouchTargetSizeCheck">


            </EditText>


            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/shapeableImageView16"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginStart="28dp"
                android:src="@drawable/upin"
                app:layout_constraintBottom_toBottomOf="@+id/textInputEditText"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/textInputEditText">

            </com.google.android.material.imageview.ShapeableImageView>
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>
</layout>